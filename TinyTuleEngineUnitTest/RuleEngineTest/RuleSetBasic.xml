<rules>
  <rule name="IsGasGuzzler" appliesto="TinyRuleEngineTest.CarDTO">
    <or>
      <and>
        <ruleitem membername="Year" operator="GreaterThanOrEqual" targetvalue="2010"/>
        <ruleitem membername="Make" operator="Equals" targetvalue="Ford"/>
      </and>
      <ruleitem membername="Model" operator="StartsWith" targetvalue="Ex"/>
    </or>
  </rule>
</rules>